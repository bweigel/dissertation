file = {:home/mori/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fabre et al. - 2001 - Determination of flavone, flavonol, and flavanone aglycones by negative ion liquid chromatography electrospray ion.pdf:pdf;:home/mori/Downloads/literatur/1-s2.0-S1044030501002264-main.pdf:pdf},
isbn = {1044-0305},
issn = {10440305},
journal = {Journal of the American Society for Mass Spectrometry},
number = {6},
pages = {707--715},
pmid = {11401161},
title = {{Determination of flavone, flavonol, and flavanone aglycones by negative ion liquid chromatography electrospray ion trap mass spectrometry}},
