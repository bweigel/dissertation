journal = {Acta Crystallographica Section D: Biological Crystallography},
keywords = {Macromolecular crystallography,Maximum likelihood,Molecular replacement},
month = jan,
number = {1},
pages = {32--41},
pmid = {17164524},
title = {{Solving structures of protein complexes by molecular replacement with Phaser}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2483468\&tool=pmcentrez\&rendertype=abstract},
volume = {63},
