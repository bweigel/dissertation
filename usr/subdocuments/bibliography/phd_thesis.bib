file = {:home/mori/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Santos et al. - 2014 - A SAR and QSAR study of new artemisinin compounds with antimalarial activity.pdf:pdf},
issn = {14203049},
journal = {Molecules},
keywords = {Antimalarial activity,Artemisinin,HF/6-31G,MEPs,Molecular docking,QSAR,SAR},
number = {1},
pages = {367--399},
pmid = {24381053},
title = {{A SAR and QSAR study of new artemisinin compounds with antimalarial activity}},
volume = {19},
year = {2014}
}
