file = {:home/mori/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chen et al. - 2010 - MolProbity all-atom structure validation for macromolecular crystallography.pdf:pdf},
isbn = {0907-4449},
issn = {09074449},
journal = {Acta Crystallographica Section D: Biological Crystallography},
keywords = {All-atom contacts,Automated correction,Clashscore,Database improvement,KiNG,Model quality,Ramachandran plots,Ribose pucker,Side-chain rotamers,Systematic errors},
month = jan,
number = {1},
pages = {12--21},
pmid = {20057044},
title = {{MolProbity: All-atom structure validation for macromolecular crystallography}},
