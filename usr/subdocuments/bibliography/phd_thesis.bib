journal = {Journal of molecular biology},
keywords = {Acyl Coenzyme A,Acyl Coenzyme A: chemistry,Amino Acid Sequence,Binding Sites,Caffeic Acids,Caffeic Acids: chemistry,Catalysis,Chromones,Chromones: chemistry,Crystallography,Glucose,Glucose: chemistry,Glucosides,Glucosides: chemistry,Magnesium,Magnesium: chemistry,Mesembryanthemum,Mesembryanthemum: enzymology,Methylation,Molecular Sequence Data,Plant Proteins,Plant Proteins: chemistry,Plant Proteins: genetics,Protein Conformation,Protein O-Methyltransferase,Protein O-Methyltransferase: chemistry,Protein O-Methyltransferase: genetics,Quercetin,Quercetin: chemistry,Substrate Specificity,Substrate Specificity: genetics,X-Ray},
month = apr,
number = {1},
pages = {154--64},
pmid = {18342334},
title = {{Biochemical and structural analysis of substrate promiscuity in plant Mg2+-dependent O-methyltransferases.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18342334},
volume = {378},
year = {2008}
}
