journal = {Acta crystallographica. Section D, Biological crystallography},
keywords = {Automatic Data Processing,Automation, Laboratory,Crystallization,Crystallography, X-Ray,Crystallography, X-Ray: instrumentation,Crystallography, X-Ray: methods,Nucleic Acids,Nucleic Acids: chemistry,Proteins,Proteins: chemistry,Quality Control,Reproducibility of Results,Research Design,Software},
month = jan,
