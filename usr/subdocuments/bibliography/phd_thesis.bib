@article{Vogt2004,
abstract = {A recently discovered, S-adenosyl-L-methionine and bivalent cation-dependent O-methyltransferase from the ice plant, Mesembryanthemum crystallinum, is involved in the methylation of various flavonoid and phenylpropanoid conjugates. Differences in regiospecificity as well as altered kinetic properties of the recombinant as compared to the native plant O-methyltransferase can be attributed to differences in the N-terminal part of the protein. Upon cleavage of the first 11 amino acids, the recombinant protein displays essentially the same substrate specificity as observed earlier for the native plant enzyme. Product formation of the newly designed, truncated recombinant enzyme is consistent with light-induced accumulation of methylated flavonoid conjugates in the ice plant. Therefore, substrate affinity and regiospecificity of an O-methyltransferase in vivo and in vitro can be controlled by cleavage of an N-terminal domain.},
author = {Vogt, Thomas},
doi = {10.1016/S0014-5793(04)00163-2},
file = {:home/bweigel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Vogt - 2004 - Regiospecificity and kinetic properties of a plant natural product O-methyltransferase are determined by its N-terminal do.pdf:pdf},
issn = {0014-5793},
journal = {FEBS letters},
keywords = {Flavonoids,Flavonoids: metabolism,Flavonoids: radiation effects,Kinetics,Light,Mesembryanthemum,Mesembryanthemum: enzymology,Methylation,Methyltransferases,Methyltransferases: chemistry,Methyltransferases: metabolism,Protein Structure,Recombinant Proteins,Recombinant Proteins: metabolism,Sequence Deletion,Substrate Specificity,Tertiary},
month = mar,
number = {1-3},
pages = {159--62},
pmid = {15013769},
title = {{Regiospecificity and kinetic properties of a plant natural product O-methyltransferase are determined by its N-terminal domain.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15013769},
volume = {561},
year = {2004}
}
