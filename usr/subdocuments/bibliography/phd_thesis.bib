issn = {19326203},
journal = {PLoS ONE},
keywords = {Arabidopsis,Arabidopsis: genetics,Base Sequence,Cloning, Molecular,Cloning, Molecular: methods,DNA Restriction Enzymes,DNA Restriction Enzymes: metabolism,Efficiency,Escherichia coli,Escherichia coli: genetics,Genes, Plant,Genetic Vectors,Genetic Vectors: genetics,Kanamycin Resistance,Kanamycin Resistance: genetics,Models, Biological,Molecular Sequence Data,Plants, Genetically Modified,Sensitivity and Specificity,Time Factors},
month = jan,
