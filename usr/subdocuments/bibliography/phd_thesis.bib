publisher = {International Association for Food Protection},
title = {{Antimicrobial activities of tea catechins and theaflavins and tea extracts against Bacillus cereus.}},
url = {http://www.ingentaconnect.com/content/iafp/jfp/2006/00000069/00000002/art00015?token=004b17b26d4309c383a4b3b25702e7b59243f6a5e2c4066562a726e2d2954496f642f466f9e},
