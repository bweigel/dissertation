journal = {Analytica chimica acta},
keywords = {Biological Products,Biological Products: chemistry,DNA,DNA: chemistry,Glutens,Glutens: chemistry,Green Chemistry Technology,Hydrogen Bonding,Ionic Liquids,Ionic Liquids: chemistry,Magnetic Resonance Spectroscopy,Solubility,Solvents,Solvents: chemistry,Starch,Starch: chemistry,Temperature,Thermogravimetry,Viscosity},
month = mar,
pages = {61--8},
pmid = {23427801},
publisher = {Elsevier B.V.},
title = {{Natural deep eutectic solvents as new potential media for green technology.}},
