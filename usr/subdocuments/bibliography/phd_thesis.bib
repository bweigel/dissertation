title = {{Comprehensive structural and substrate specificity classification of the saccharomyces cerevisiae methyltransferome}},
volume = {6},
year = {2011}
}
@article{Boal2011,
abstract = {The radical S-adenosyl-L-methionine (SAM) enzymes RlmN and Cfr methylate 23S ribosomal RNA, modifying the C2 or C8 position of adenosine 2503. The methyl groups are installed by a two-step sequence involving initial methylation of a conserved Cys residue (RlmN Cys(355)) by SAM. Methyl transfer to the substrate requires reductive cleavage of a second equivalent of SAM. Crystal structures of RlmN and RlmN with SAM show that a single molecule of SAM coordinates the [4Fe-4S] cluster. Residue Cys(355) is S-methylated and located proximal to the SAM methyl group, suggesting the SAM that is involved in the initial methyl transfer binds at the same site. Thus, RlmN accomplishes its complex reaction with structural economy, harnessing the two most important reactivities of SAM within a single site.},
author = {Boal, Amie K and Grove, Tyler L and McLaughlin, Monica I and Yennawar, Neela H and Booker, Squire J and Rosenzweig, Amy C},
