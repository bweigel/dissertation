file = {:home/mori/Downloads/literatur/Regueiro2013.pdf:pdf},
issn = {03088146},
journal = {Food Chemistry},
keywords = {Ellagitannins,High-resolution mass spectrometry,Juglans regia L.,Orbitrap,Phenolic compounds,Walnuts},
pages = {340--348},
pmid = {24444946},
publisher = {Elsevier Ltd},
title = {{Comprehensive identification of walnut polyphenols by liquid chromatography coupled to linear ion trap-Orbitrap mass spectrometry}},
