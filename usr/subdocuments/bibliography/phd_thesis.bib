journal = {Annual review of biochemistry},
keywords = {carbohydrate-modifying enzymes,glycobiology,glycosylation,ion,nucleophilic substitution,pair mechanisms},
pages = {521--555},
pmid = {18518825},
title = {{Glycosyltransferases: structures, functions, and mechanisms.}},
volume = {77},
year = {2008}
}
