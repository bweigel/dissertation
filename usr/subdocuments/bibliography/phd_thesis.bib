@article{Dippe2014,
abstract = {A simple spectrophotometric microplate assay that allows quantification of the interaction between phospholipids and metal ions or other small cationic compounds has been developed. The assay is based on the competition of the phospholipids for the Fe(3+) ion in the purple-colored Fe(III)-$\gamma$-resorcylate complex and for other cations. To compare the binding affinities of several cation-phospholipid interactions, K0.5 values were derived from binding curves constructed by determination of the absorbance of the Fe(III)-$\gamma$-resorcylate at 490 nm as a function of the cation concentration. The assay was used to analyze the binding of lanthanide ions, calcium ions, and amines (hydrochlorides of ethanolamine, spermidine, and hexyltrimethylammonium chloride) to small unilamellar vesicles (SUVs) and mixed micelles containing anionic lipids such as phosphatidic acid and phosphatidyl-p-nitrophenol. The method was evaluated by fluorescence measurements with Eu(3+) ions as tracer. Lanthanide ions such as La(3+) and Ce(3+) ions showed K0.5 values smaller by one to two orders of magnitude compared with Ca(2+) ions. In the presence of increasing amounts of detergents such as Triton X-100, the method also reflected transitions from SUVs to micelles. The binding capacity for metal ions was higher for phospholipid-containing micelles than for the corresponding SUVs.},
author = {Dippe, Martin and Dressler, Lars and Ulbrich-Hofmann, Renate},
doi = {10.1016/j.ab.2013.10.008},
issn = {1096-0309},
journal = {Analytical biochemistry},
keywords = {Amines,Amines: chemistry,Anions,Anions: chemistry,Calcium,Calcium: chemistry,Cations,Cations: chemistry,Coordination Complexes,Coordination Complexes: chemistry,Europium,Europium: chemistry,Ferric Compounds,Ferric Compounds: chemistry,Kinetics,Micelles,Nitrophenols,Nitrophenols: chemistry,Octoxynol,Octoxynol: chemistry,Phospholipids,Phospholipids: chemistry,Spectrophotometry,Unilamellar Liposomes,Unilamellar Liposomes: chemistry},
month = jan,
pages = {54--9},
pmid = {24135656},
title = {{Fe(III)-resorcylate as a spectrophotometric probe for phospholipid-cation interactions.}},
url = {http://www.sciencedirect.com/science/article/pii/S0003269713004867},
volume = {445},
