doi = {10.1021/ja077104v},
file = {:home/mori/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Osborne et al. - 2008 - In situ generation of a bisubstrate analogue for protein arginine methyltransferase 1.pdf:pdf},
