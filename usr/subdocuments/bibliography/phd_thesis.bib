pages = {12--21},
pmid = {20057044},
title = {{MolProbity: all-atom structure validation for macromolecular crystallography.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2803126\&tool=pmcentrez\&rendertype=abstract},
