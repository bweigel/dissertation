journal = {Applied and environmental microbiology},
number = {8},
pages = {2852--2860},
pmid = {25681184},
title = {{Arsenic methylation and volatilization by arsenite S-adenosylmethionine methyltransferase in Pseudomonas alcaligenes NBRC14159.}},
url = {http://aem.asm.org/lookup/doi/10.1128/AEM.03804-14},
