journal = {Journal of the American Chemical Society},
number = {20},
pages = {7648--7651},
pmid = {21539310},
title = {{Labeling substrates of protein arginine methyltransferase with engineered enzymes and matched S-adenosyl-l-methionine analogues}},
volume = {133},
year = {2011}
}
