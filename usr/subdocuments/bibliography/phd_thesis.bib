issn = {10177825},
journal = {Journal of Microbiology and Biotechnology},
keywords = {Flavonoids,O-Methyltransferase,Streptomyces avermitilis},
number = {9},
pages = {1359--1366},
pmid = {20890103},
title = {{Characterization of an O-methyltransferase from Streptomyces avermitilis MA-4680}},
volume = {20},
